<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Contact Me</title>
</head>
<body>
	<main>
		<div class="wp-main-content-contact">
			<div class="container">
				<div class="row">
					<div class="wp-map">
						<iframe
							src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d25042.604229878292!2d105.65315927705997!3d21.088417284446!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3134564279584b87%3A0xf71d5346979a7cf0!2zdHQuIFBow7luZywgxJBhbiBQaMaw4bujbmcsIEjDoCBO4buZaSwgVmnhu4d0IE5hbQ!5e0!3m2!1svi!2s!4v1596032806951!5m2!1svi!2s"
							width="600" height="450" frameborder="0" style="border: 0;"
							allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
					</div>
					<div class="wp-main-contact">
						<div class="contact-title">
							<p>Contact Me</p>
						</div>
						<div class="contact-content">
							<div class="contact-content--item">
								<div class="contact-icon">
									<i class="fas fa-map-marker-alt"></i>
								</div>
								<div class="contact-txt">
									<p>Phung town, Dan Phuong, Ha Noi</p>
								</div>
							</div>
							<div class="contact-content--item">
								<div class="contact-icon">
									<i class="fas fa-mobile-alt"></i>
								</div>
								<div class="contact-txt">
									<p>+ 0963042088</p>
								</div>
							</div>
							<div class="contact-content--item">
								<div class="contact-icon">
									<i class="far fa-envelope"></i>
								</div>
								<div class="contact-txt">
									<p>buiduchieuuet16@gmail.com</p>
								</div>
							</div>
						</div>
					</div>
					<div class="wp-main-contact">
						<div class="contact-title">
							<p>Drop A Mail</p>
						</div>
						<div class="wp-mail-send">
							<input type="text" class="mail-send--blank mail-send--info"
								placeholder="Enter your name" /> <input type="text"
								class="mail-send--blank mail-send--info"
								placeholder="Your email" /> <input type="text"
								class="mail-send--blank mail-send--info" placeholder="Subject" />
							<textarea class="mail-send--blank mail-send--txt"
								placeholder="Type your message"></textarea>
							<div class="button-submit">
								<button type="submit">Send Message</button>
							</div>
						</div>

					</div>
				</div>
			</div>
		</div>
	</main>
</body>
</html>